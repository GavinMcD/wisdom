<!DOCTYPE html>
<html>
<head>
  <title>Wisdom</title>
  <%= stylesheet_link_tag    "application" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>

<% flash.each do |name, msg| %>
  <%= content_tag :div, msg, :id => "flash_#{name}" %>
<% end %>	
	
<%= yield %>

<%= debug(params) if Rails.env.development? %>

</body>
</html>
